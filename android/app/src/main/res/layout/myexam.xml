<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:background="@drawable/bodr_bottom"
        android:stretchColumns="*"
        android:shrinkColumns="1"
        android:gravity="center">
        <TableRow android:layout_height="fill_parent">
            <TextView
                android:layout_width="110dp"
                android:layout_height="fill_parent"
                android:text="四川省成都市"
                android:textColor="@color/gray"
                android:paddingLeft="15dp"
                android:textSize="18dp" />
            <com.cxhd.freight.custom.CustomFontTextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text=""
                android:textColor="@color/nomalGray"
                android:textSize="18dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text="全国"
                android:textColor="@color/gray"
                android:textSize="18dp" />
        </TableRow>
    </TableLayout>
    <com.cxhd.freight.view.MyListView
        android:id="@+id/myexam_listview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:cacheColorHint="#00000000"
        android:divider="@null"
        android:dividerHeight="0px"
        android:fadingEdge="none"
        android:footerDividersEnabled="false" />

</LinearLayout>